{
  "name": "icrules",
  "version": "0.1.1",
  "scripts": {
    "test": "jest --config ./jest.config.js  --coverage",
    "build": "npm run build --workspaces --if-present",
    "editor": "npm run editor --workspace=editor",
    "gendocs": "npm run gendocs --workspace=@icrules/core",
    "clean": "npm run clean --workspaces --if-present",
    "examples": "npm run examples --workspace=@icrules/examples",
    "watch": "concurrently \"npm run watch --workspace=@icrules/core\" \"npm run watch --workspace=@icrules/editor\"",
    "build:clean": "npm run build:clean --workspaces --if-present"
  },
  "keywords": [
    "rules",
    "jsonrules",
    "rulesengine",
    "rules editor",
    "integrated"
  ],
  "author": "Chad Neff <ccneff@gmail.com> (https://github.com/ineffably)",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "babel-loader": "^9.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "concurrently": "^8.1.0",
    "eslint": "^8.42.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "lerna": "^8.1.2",
    "ts-jest": "^29.1.0",
    "ts-jest-resolver": "^2.0.1"
  },
  "workspaces": [
    "packages/core",
    "packages/editor",
    "packages/examples"
  ]
}
